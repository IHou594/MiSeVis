library(survival)
library(survminer)
setwd("C:\\Users\\ianho\\OneDrive\\Desktop\\BioinformaticStuff\\RCode\\MiSeVis")
data_dir <- "C:\\Users\\ianho\\OneDrive\\Desktop\\BioinformaticStuff\\RCode\\MiSeVis"
graph_dir <- "C:\\Users\\ianho\\OneDrive\\Desktop\\BioinformaticStuff\\RCode\\MiSeVis"
data_files <- list.files(path = data_dir, pattern = "*.tsv", full.names = TRUE)
for(file in data_files) {
data <- read.delim(file)
gene_name <- names(data)[ncol(data)]
surv_obj <- Surv(data$OS.time, data$OS)
km_fit <- survfit(surv_obj ~ get(gene_name), data = data)
new_gene_name <- gsub('.', '', gene_name) #somatic.mutation..SNP.and.INDEL....MC3.public.version.
plot_km<- ggsurvplot(km_fit, data = data,
title = paste0("Effect of ", new_gene_name, " expression level on OV patient survival"),
xlab = "Time in Days",
#legend.lab = "Expression",
pval = TRUE,
ggtheme = theme_bw(),
font.main = c(16, "bold", "black")
)
ggsave(paste0(data_dir, "/", new_gene_name, "-KM-Exp_.png"), plot_km$plot,
limitsize = FALSE, width = 3000, height = 2000, units = "px")
}
